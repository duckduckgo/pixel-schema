{
    "w_wide_import_summary": {
        "description": "Fires at the conclusion of an import process to provide a summary of all imported data. For multi-stage imports (e.g., browser + CSV), this fires only once at the end with combined results.",
        "owners": [
            "tester"
        ],
        "meta": {
            "type": "w_wide_import_summary",
            "version": "1.0.0"
        },
        "app": {
            "name": {
                "type": "string",
                "description": "The name of the application",
                "enum": [
                    "Windows"
                ]
            },
            "version": {
                "type": "string",
                "description": "The version of the application",
                "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$"
            }
        },
        "global": {
            "platform": {
                "type": "string",
                "description": "The platform the app is running on",
                "enum": [
                    "Windows"
                ]
            },
            "type": {
                "type": "string",
                "description": "The type of application",
                "enum": [
                    "app"
                ]
            },
            "sample_rate": {
                "type": "number",
                "description": "Sample rate for this event"
            }
        },
        "feature": {
            "name": {
                "type": "string",
                "description": "The feature name for this event",
                "enum": [
                    "browser-import"
                ]
            },
            "status": {
                "type": "string",
                "description": "The overall status of the operation",
                "enum": [
                    "SUCCESS",
                    "FAILURE",
                    "CANCELLED"
                ]
            },
            "data": {
                "ext": {
                    "import_from": {
                        "type": "string",
                        "description": "The source of the import data. For multi-stage imports, this is the original browser source.",
                        "enum": [
                            "Google Chrome",
                            "Microsoft Edge",
                            "Mozilla Firefox",
                            "Brave",
                            "Vivaldi",
                            "Chromium",
                            "Bitwarden",
                            "CSV Logins File",
                            "HTML Bookmarks File",
                            "LastPass",
                            "1Password",
                            "1Password 7"
                        ]
                    },
                    "total_items_bucketed": {
                        "type": "integer",
                        "description": "Total number of items imported, bucketed to preserve privacy",
                        "enum": [
                            1,
                            5,
                            10,
                            20,
                            50,
                            100,
                            500,
                            1000
                        ]
                    },
                    "bookmark_count_bucketed": {
                        "type": "integer",
                        "description": "Number of bookmarks imported, bucketed to preserve privacy",
                        "enum": [
                            1,
                            5,
                            10,
                            20,
                            50,
                            100,
                            500,
                            1000
                        ]
                    },
                    "credential_count_bucketed": {
                        "type": "integer",
                        "description": "Number of credentials imported, bucketed to preserve privacy",
                        "enum": [
                            1,
                            5,
                            10,
                            20,
                            50,
                            100,
                            500,
                            1000
                        ]
                    },
                    "bookmark_failure_count_bucketed": {
                        "type": "integer",
                        "description": "Number of errors encountered during import, bucketed to preserve privacy",
                        "enum": [
                            1,
                            5,
                            10,
                            20,
                            50,
                            100,
                            500,
                            1000
                        ]
                    },
                    "credential_failure_count_bucketed": {
                        "type": "integer",
                        "description": "Number of errors encountered during import, bucketed to preserve privacy",
                        "enum": [
                            1,
                            5,
                            10,
                            20,
                            50,
                            100,
                            500,
                            1000
                        ]
                    }
                }
            }
        },
        "context": {
            "name": {
                "type": "string",
                "description": "Context name for the event",
                "enum": [
                    "onboarding",
                    "settings"
                ]
            }
        }
    }
}