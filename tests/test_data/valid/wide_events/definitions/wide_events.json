{
    "w_wide_import_summary": {
        "description": "Fires at the conclusion of an import process to provide a summary of all imported data. For multi-stage imports (e.g., browser + CSV), this fires only once at the end with combined results.",
        "owners": ["tester"],
        "meta": {
            "type": "w_wide_import_summary",
            "version": "0.0"
        },
        "context": ["onboarding", "settings"],
        "feature": {
            "name": "browser-import",
            "status": ["SUCCESS", "FAILURE", "CANCELLED"],
            "data": {
                "ext": {
                    "import_from": {
                        "type": "string",
                        "description": "The source of the import data. For multi-stage imports, this is the original browser source.",
                        "enum": [
                            "Google Chrome",
                            "Microsoft Edge",
                            "Mozilla Firefox",
                            "Brave",
                            "Vivaldi",
                            "Chromium",
                            "Bitwarden",
                            "CSV Logins File",
                            "HTML Bookmarks File",
                            "LastPass",
                            "1Password",
                            "1Password 7"
                        ]
                    },
                    "total_items_bucketed": {
                        "type": "integer",
                        "description": "Total number of items imported, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    },
                    "bookmark_count_bucketed": {
                        "type": "integer",
                        "description": "Number of bookmarks imported, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    },
                    "credential_count_bucketed": {
                        "type": "integer",
                        "description": "Number of credentials imported, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    },
                    "bookmark_failure_count_bucketed": {
                        "type": "integer",
                        "description": "Number of errors encountered during import, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    },
                    "credential_failure_count_bucketed": {
                        "type": "integer",
                        "description": "Number of errors encountered during import, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    }
                }
            }
        }
    },
    "w_wide_import_bookmarks": {
        "description": "Fires when bookmarks are imported, providing metrics about the bookmark import component of an import operation.",
        "owners": ["tester"],
        "meta": {
            "type": "w_wide_import_bookmarks",
            "version": "0.1"
        },
        "context": ["onboarding.import", "settings.import"],
        "feature": {
            "name": "browser-import-bookmarks",
            "status": ["SUCCESS", "FAILURE"],
            "data": {
                "ext": {
                    "import_from": {
                        "type": "string",
                        "description": "The source of the bookmark import",
                        "enum": [
                            "Google Chrome",
                            "Microsoft Edge",
                            "Mozilla Firefox",
                            "Brave",
                            "Vivaldi",
                            "Chromium",
                            "HTML Bookmarks File"
                        ]
                    },
                    "item_count_bucketed": "itemCountBucketed",
                    "bookmark_failure_count_bucketed": {
                        "type": "integer",
                        "description": "If status is FAILURE, number of failed bookmark imports, bucketed to preserve privacy",
                        "enum": [0, 1, 2, 3, 4, 5]
                    }
                }
            }
        }
    },
    "w_wide_import_credentials": {
        "description": "Fires when credentials are imported, providing metrics about the credential import component of an import operation.",
        "owners": ["tester"],
        "meta": {
            "type": "w_wide_import_credentials",
            "version": "0.1"
        },
        "context": ["onboarding.import", "settings.import"],
        "feature": {
            "name": "browser-import-credentials",
            "status": ["SUCCESS", "FAILURE"],
            "data": {
                "ext": {
                    "import_from": {
                        "type": "string",
                        "description": "The source of the credential import",
                        "enum": [
                            "Google Chrome",
                            "Microsoft Edge",
                            "Brave",
                            "Vivaldi",
                            "Chromium",
                            "Bitwarden",
                            "CSV Logins File",
                            "LastPass",
                            "1Password",
                            "1Password 7"
                        ]
                    },
                    "item_count_bucketed": "itemCountBucketed",
                    "credential_failure_count_bucketed": {
                        "type": "integer",
                        "description": "If status is FAILURE, number of failed credential imports, bucketed to preserve privacy",
                        "enum": [0, 1, 2, 3, 4, 5]
                    },
                    "signal": {
                        "type": "number",
                        "description": "Made up signal",
                        "minimum": 0,
                        "maximum": 10
                    }
                }
            }
        }
    },
    "w_wide_import_timeout": {
        "description": "Fires when an import operation takes longer than expected and times out.",
        "owners": ["tester"],
        "meta": {
            "type": "w_wide_import_timeout",
            "version": "1.0"
        },
        "context": ["onboarding", "settings"],
        "feature": {
            "name": "browser-import",
            "status": ["FAILURE"],
            "data": {
                "ext": {
                    "import_from": {
                        "type": "string",
                        "description": "The source of the import that timed out",
                        "enum": [
                            "Google Chrome",
                            "Microsoft Edge",
                            "Mozilla Firefox",
                            "Brave",
                            "Vivaldi",
                            "Chromium",
                            "Bitwarden",
                            "CSV Logins File",
                            "HTML Bookmarks File",
                            "LastPass",
                            "1Password",
                            "1Password 7"
                        ]
                    },
                    "total_items_bucketed": {
                        "type": "integer",
                        "description": "Number of items that were being imported when the timeout occurred, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    }
                }
            }
        }
    },
    "w_wide_import_cancelled": {
        "description": "Fires when an import operation is cancelled by the user.",
        "owners": ["tester"],
        "meta": {
            "type": "w_wide_import_cancelled",
            "version": "1.0"
        },
        "context": ["onboarding", "settings"],
        "feature": {
            "name": "browser-import",
            "status": ["CANCELLED"],
            "data": {
                "ext": {
                    "import_from": {
                        "type": "string",
                        "description": "The source of the import that was cancelled",
                        "enum": [
                            "Google Chrome",
                            "Microsoft Edge",
                            "Mozilla Firefox",
                            "Brave",
                            "Vivaldi",
                            "Chromium",
                            "Bitwarden",
                            "CSV Logins File",
                            "HTML Bookmarks File",
                            "LastPass",
                            "1Password",
                            "1Password 7"
                        ]
                    },
                    "total_items_bucketed": {
                        "type": "integer",
                        "description": "Number of items that were being imported when cancelled, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    },
                    "nested_object": {
                        "type": "object",
                        "description": "A nested object to test deep structures",
                        "properties": {
                            "inner_property": {
                                "type": "string",
                                "description": "An inner property within the nested object",
                                "enum": ["value1", "value2", "value3"]
                            }
                        }
                    }
                }
            }
        }
    }
}
