    "w_wide_import_timeout": {
        "description": "Fires when an import operation takes longer than expected and times out.",
        "owners": ["jhlouw-dev"],
        "meta": {
            "type": "w_wide_import_timeout",
            "version": "0.0"
        },
        "context": ["onboarding", "settings"],
        "feature": {
            "name": "browser-import",
            "status": ["FAILURE"],
            "data": {
                "failure_detail": {
                    "type": "string",
                    "description": "Failure detail for timeout",
                    "enum": ["TimeoutFailure"]
                },
                "ext": {
                    "import_from": {
                        "type": "string",
                        "description": "The source of the import that timed out",
                        "enum": [
                            "Google Chrome",
                            "Microsoft Edge",
                            "Mozilla Firefox",
                            "Brave",
                            "Vivaldi",
                            "Chromium",
                            "Bitwarden",
                            "CSV Logins File",
                            "HTML Bookmarks File",
                            "LastPass",
                            "1Password",
                            "1Password 7"
                        ]
                    },
                    "total_items_bucketed": {
                        "type": "integer",
                        "description": "Number of items that were being imported when the timeout occurred, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    }
                }
            }
        }
    },
