{
    "w_wide_import_summary": {
        "description": "Fires at the conclusion of an import process to provide a summary of all imported data. For multi-stage imports (e.g., browser + CSV), this fires only once at the end with combined results.",
        "owners": ["jhlouw-dev"],
        "meta": {
            "type": "w_wide_import_summary",
            "version": "0.0"
        },
        "context": ["onboarding", "settings"],
        "feature": {
            "name": "browser-import",
            "data": {
                "latency_ms_bucketed": {
                    "type": "integer",
                    "description": "How long the import operation took in milliseconds, bucketed to preserve privacy",
                    "enum": [1, 5, 10, 20, 50, 100, 500, 1000, 5000, 10000]
                },
                "ext": {
                    "import_from": {
                        "type": "string",
                        "description": "The source of the import data. For multi-stage imports, this is the original browser source.",
                        "enum": [
                            "Google Chrome",
                            "Microsoft Edge",
                            "Mozilla Firefox",
                            "Brave",
                            "Vivaldi",
                            "Chromium",
                            "Bitwarden",
                            "CSV Logins File",
                            "HTML Bookmarks File",
                            "LastPass",
                            "1Password",
                            "1Password 7"
                        ]
                    },
                    "total_items_bucketed": {
                        "type": "integer",
                        "description": "Total number of items imported, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    },
                    "bookmark_count_bucketed": {
                        "type": "integer",
                        "description": "Number of bookmarks imported, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    },
                    "credential_count_bucketed": {
                        "type": "integer",
                        "description": "Number of credentials imported, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    },
                    "bookmark_failure_count_bucketed": {
                        "type": "integer",
                        "description": "Number of errors encountered during import, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    },
                    "credential_failure_count_bucketed": {
                        "type": "integer",
                        "description": "Number of errors encountered during import, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    }
                },
                "failure_detail": {
                    "type": "string",
                    "description": "Details about the error if one occurred",
                    "enum": ["DecryptionFailed", "TimeoutFailure", "FileFormatFailure", "AccessDenied", "UnknownFailure"]
                }
            }
        }
    },
    "w_wide_import_bookmarks": {
        "description": "Fires when bookmarks are imported, providing metrics about the bookmark import component of an import operation.",
        "owners": ["jhlouw-dev"],
        "meta": {
            "type": "w_wide_import_bookmarks",
            "version": "0.0"
        },
        "feature": {
            "name": "browser-import-bookmarks",
            "status": ["SUCCESS", "FAILURE"],
            "data": {
                "latency_ms_bucketed": {
                    "type": "integer",
                    "description": "How long the bookmark import took in milliseconds, bucketed to preserve privacy",
                    "enum": [1, 5, 10, 20, 50, 100, 500, 1000, 5000, 10000]
                },
                "ext": {
                    "import_from": {
                        "type": "string",
                        "description": "The source of the bookmark import",
                        "enum": [
                            "Google Chrome",
                            "Microsoft Edge",
                            "Mozilla Firefox",
                            "Brave",
                            "Vivaldi",
                            "Chromium",
                            "HTML Bookmarks File"
                        ]
                    },
                    "item_count_bucketed": {
                        "type": "integer",
                        "description": "Number of bookmarks imported, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    },
                    "bookmark_failure_count_bucketed": {
                        "type": "integer",
                        "description": "If status is FAILURE, number of failed bookmark imports, bucketed to preserve privacy",
                        "enum": [1, 5, 10, 20, 50, 100, 500, 1000]
                    }
                },
                "failure_detail": {
                    "type": "string",
                    "description": "Details about the error if one occurred",
                    "enum": ["FileFormatFailure", "AccessDenied", "ParsingFailure", "UnknownFailure"]
                }
            }
        }
    },
